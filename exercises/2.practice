//输入三个数并输出最小值
int main()
{
    int a,b,c,temp,min;
    printf("请输入三个数：");
    scanf_s("%d,%d,%d",&a,&b,&c);
    temp=a>b?b:a;
    min=temp>c?c:temp;
    printf("最小值为：%d",min);
    return 0;
}


//随机产生两个三位的自然数，并求其平均值
int main()
{
    int x,y;
    double average;
    x=rand()%900+100;
    y=rand()%900+100;
    average=(x+y)/2.0;
    printf("%.2lf\n",average);
    return 0;
}


